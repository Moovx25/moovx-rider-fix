{
  "expo": {
    "name": "moovx-rider",
    "slug": "moovx-rider",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/moovxlogo.png",
    "scheme": "moovxrider",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/moovxlogo.png",
        "backgroundColor": "#0000FF"
      },
      "package": "com.moovx.rider"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/moovxlogo.png"
    },
    "splash": {
      "image": "./assets/images/bg1.png",
      "resizeMode": "cover",
      "backgroundColor": "#0000FF"
    },
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "5f050d47-ef27-4cb4-9c37-026b08608074"
      }
    }
  }
}
